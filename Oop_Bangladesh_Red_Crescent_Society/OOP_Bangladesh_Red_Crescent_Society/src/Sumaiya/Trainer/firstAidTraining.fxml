      <TextField fx:id="trainerNameTextField" layoutX="182.0" layoutY="206.0" prefHeight="31.0" prefWidth="146.0" />
      <Label layoutX="371.0" layoutY="277.0" text="Course Duration" textFill="#f5efef">
