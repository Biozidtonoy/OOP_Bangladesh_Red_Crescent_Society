<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="Sumya.MediaAndPublication.ReportController">
   <children>
      <Button layoutX="44.0" layoutY="207.0" mnemonicParsing="false" onAction="#addreportonclick" text="add report" />
      <TableView layoutX="158.0" layoutY="164.0" prefHeight="200.0" prefWidth="415.0">
        <columns>
          <TableColumn fx:id="datecolumn" onEditCommit="#datecolumnonedit" prefWidth="75.0" text="date" />
          <TableColumn fx:id="reportcolumn" onEditCommit="#reportcolumnonedit" prefWidth="339.0" text="report" />
        </columns>
      </TableView>
      <Label layoutX="30.0" layoutY="31.0" text="Report" />
      <Label layoutX="30.0" layoutY="75.0" text="Date" />
      <DatePicker fx:id="datepicker" layoutX="105.0" layoutY="71.0" />
      <TextField fx:id="reporttextfield" layoutX="96.0" layoutY="27.0" prefHeight="25.0" prefWidth="487.0" />
      <Button layoutX="55.0" layoutY="279.0" mnemonicParsing="false" onAction="#backbuttononclick" text="back" />
   </children>
</AnchorPane>
